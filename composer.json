            "aliases": {}
        }
    },
    "homepage": "https://nexamerchant.github.io/docs/",
    "docs-url": "nexamerchant.github.io/CheckoutCod/",
    "github": "https://github.com/NexaMerchant/CheckoutCod",
    "minimum-stability": "stable"
