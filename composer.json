{
    "name": "nexa-merchant/checkoutcod",
    "license": "MIT",
    "type": "library",
    "description" : "CheckoutCod Apps for Nexa Merchant",
    "keywords": ["checkoutcod", "nexamerchant", "laravel", "ecommerce", "payment", "cod", "cashondelivery", "paymentgateway"],
    "authors": [
        {
            "name": "Steve",
            "email": "nice.lizhi@gmail.com"
        }
    ],
    "require": {
        "nexa-merchant/apis": "2.0.2",
        "laravel/framework": "~6.0||~7.0||~8.0||~9.0||~10.0||~11.0"
    },
    "require-dev": {
        "nexa-merchant/apis": "2.0.2",
        "phpunit/phpcov": ">=6.0",
        "phpunit/phpunit": ">=8.0",
        "laravel/framework": "~6.0||~7.0||~8.0||~9.0||~10.0||~11.0"
    },
    "autoload": {
        "psr-4": {
            "NexaMerchant\\CheckoutCod\\": "src/"
